{
    "rickshaw-benchmark": {
        "schema": {
            "version": "2020.05.18"
        }
    },
    "benchmark": "fio",
    "controller" : {
        "post-script" : "%bench-dir%/sysjitter-post-process"
    },
    "client" : {
        "files-from-controller": [
            {
                "src": "%bench-dir%/sysjitter-start",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%/sysjitter-get-runtime",
                "dest": "/usr/bin/"
            }
        ],
        "runtime" : "sysjitter-get-runtime",
        "start" : "sysjitter-start"
    }
}
